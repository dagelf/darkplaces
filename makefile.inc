SDLCONFIG_MACOSXCFLAGS=$(SDLCONFIG_UNIXCFLAGS)
SDLCONFIG_MACOSXLIBS=$(SDLCONFIG_UNIXLIBS)
SDLCONFIG_MACOSXSTATICLIBS=$(SDLCONFIG_UNIXSTATICLIBS)
STRIP?=strip


